{
  "name": "f0-agent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3030",
    "dev:web": "next dev -p 3030",
    "dev:desktop": "pnpm --filter @f0/desktop dev",
    "dev:all": "pnpm run cleanup && concurrently -k -n WEB,DESKTOP \"pnpm dev:web\" \"pnpm dev:desktop\"",
    "build": "next build",
    "build:web": "next build",
    "build:desktop": "pnpm --filter @f0/desktop build",
    "build:mobile:android": "cd apps/mobile && flutter build apk --debug",
    "build:all": "pnpm run build:web && pnpm run build:desktop",
    "start": "next start -p 3030",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "cleanup": "bash scripts/cleanup.sh",
    "ext:test": "./scripts/test-all-extensions.sh",
    "ext:doctor": "pnpm tsx scripts/ext-doctor.ts",
    "ext:chaos": "pnpm tsx scripts/chaos-test-simple.ts",
    "ext:validate": "pnpm tsx scripts/ext-validate.ts",
    "smoke-tests": "./scripts/smoke-tests.sh",
    "go-live": "./scripts/go-live.sh",
    "seed:emulator": "FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 npx tsx tools/seedEmulator.ts"
  },
