{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist-electron",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["electron/**/*", "indexer/**/*"]
}
